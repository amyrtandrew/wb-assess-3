{% extends 'base.html.njk' %}

  </head>
  {% block header %}

 <h1>Welcome, {{ name }}</h1>

 {% endblock header %}

  <body>
    {% block body %}

    {% for fossils in mostLiked %}
    <div class="fossil-div">
        <h3>{{ name }}, this is a {{ fossils.name }}</h3>
       <h2>Liked {{ fossils.num_likes }} times.</h2>
        <img src="{{ fossils.img }}" />
      {% endfor %}
    </div>
  <form id="dino-type" action="/like-fossil" method="post">
    <select>
      <option value="aust">Australopithecus</option>
      <option value="quetz">Quetzal</option>
      <option value="steg">Stegosaurus</option>
      <option value="trex">Tyrannosaurus Rex</option>
      <input type="submit" />
    </select>
  </form>
   {% endblock body %}
  </body>


